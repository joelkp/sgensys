//#include "osc.h"

enum {
  MGS_TYPE_WAIT = 0,
  MGS_TYPE_SIN,
  MGS_TYPE_SQR,
  MGS_TYPE_TRI,
  MGS_TYPE_SAW
};

enum {
  MGS_MODE_CENTER = 1|2,
  MGS_MODE_LEFT   = 1,
  MGS_MODE_RIGHT  = 2
};

typedef struct MGSProgramNode {
  struct MGSProgramNode *next;
  uchar type, mode;
  float amp, delay, time, freq;
} MGSProgramNode;

struct MGSProgram {
  MGSProgramNode *steps;
  uint stepc;
  uint componentc;
};
